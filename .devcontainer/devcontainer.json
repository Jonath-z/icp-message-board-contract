{
    "name": "ICP Message Board Contract",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "privileged": true,
    "securityOpt": [
        "seccomp=unconfined",
        "label=disabled"
    ],
    "runArgs": [
        "--cgroupns=host",
        "--device=/dev/fuse"
    ],
    "forwardPorts": [
        4943
    ],
    "portsAttributes": {
        "4943": {
            "label": "dfx",
            "onAutoForward": "ignore"
        }
    },
    "remoteUser": "root"
}